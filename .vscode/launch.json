{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "ng serve",
        "type": "firefox",
        "request": "launch",
        "firefoxExecutable": "C:\\Program Files\\Firefox Developer Edition\\firefox.exe",
        "preLaunchTask": "npm: start",
        "url": "http://localhost:4200/",
        "webRoot": "${workspaceFolder}",
        "pathMappings": [
          {
            "url": "webpack:///src",
            "path": "${workspaceFolder}/src"
          },
          {
            "url": "webpack:///src",
            "path": "${workspaceFolder}/projects/TAW0/src"
          },
          {
            "url": "webpack:///src",
            "path": "${workspaceFolder}/projects/myreactivematerial/src"
          }
        ]
      },
      {
        "name": "ng test",
        "type": "firefox",
        "request": "launch",
        "url": "http://localhost:9876/debug.html",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "ng e2e",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/protractor/bin/protractor",
        "protocol": "inspector",
        "args": [
          "${workspaceFolder}/e2e/protractor.conf.js"
        ]
      }
    ]
  }